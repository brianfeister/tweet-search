        {
          "component": "ui/TweetButton",
          "text": "tweet-search: Use this to recursively count all tweets and retweets, bypasses Twitter search API rate limits until you hit your limit anyway...",
          "url": null
        },
        {
          "component": "ui/GithubButton",
          "user": "brianfeister",
          "repo": "tweet-search"
        }
      ]
    },
    {
      "component": "article",
      "metadata": {
        "source": "github.readme"
      },
      "html": "\n\n"
    },
    {
      "component": "footer",
      "links": [
        {
          "href": "https://github.com/brianfeister/tweet-search",
          "text": "GitHub"
        },
        {
          "href": "https://github.com/brianfeister",
          "text": "brianfeister"
        }
      ]
    }
  ]
